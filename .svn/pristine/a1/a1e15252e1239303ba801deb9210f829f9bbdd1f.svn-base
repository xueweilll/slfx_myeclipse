package com.benqzl.pojo.system;

import java.util.Date;

public class AlarmControl {
	
	public AlarmControl(){
		isAC = false;
	}
	
	private boolean isAC;
	
	private Date date;
	
	public boolean getIsAC() {
		return isAC;
	}
	
	public void setIsAC(boolean isAC) {
		this.isAC = isAC;
	}
	
	public Date getDate() {
		return date;
	}
	
	public void setDate(Date date) {
		this.date = date;
	}
	
}
